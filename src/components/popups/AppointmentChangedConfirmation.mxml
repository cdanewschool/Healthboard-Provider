<?xml version="1.0" encoding="utf-8"?>
<s:TitleWindow 
	xmlns:fx="http://ns.adobe.com/mxml/2009" 
	xmlns:s="library://ns.adobe.com/flex/spark" 
	xmlns:mx="library://ns.adobe.com/flex/mx" 
	width="317" height="107"
	controlBarVisible="true" 
	skinClass="skins.general.myHeaderlessTitleWindow"
	creationComplete="init()">
	
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	
	<fx:Script>
		<![CDATA[
			import models.UserModel;
			
			import mx.events.CloseEvent;
			import mx.managers.PopUpManager;
			
			[Bindable] public var user:UserModel;
			
			private function close():void
			{
				dispatchEvent( new CloseEvent( Event.CLOSE ) );
				
				PopUpManager.removePopUp( this );
			}
			
			private function init():void
			{
				controlBarVisible = false;
			}
		]]>
	</fx:Script>
	
	<s:layout>
		<s:VerticalLayout paddingTop="15" gap="15" horizontalAlign="center" />
	</s:layout>
	
	<s:Label text="Your Appointment Has Been Changed" styleName="darkGrayBold16" />
	
	<s:Label text="Your Appointment Has Been Changed." styleName="grayText11" />
	
	<s:HGroup horizontalAlign="center" width="100%">
		<s:Button label="Close" click="close()" />
	</s:HGroup>
	
</s:TitleWindow>
